<h1>Login User</h1>
<div class="container">
    <form>
        <div class="form-group">
            <label for="Name">User Name</label>
            <input type="text" class="form-control" id="Username" placeholder="User Name" />
            <span style="color:red" class="error">Valid Username Requiere</span>
        </div>
        <div class="form-group">
            <label for="Name">Password</label>
            <input type="password" class="form-control" id="Password" placeholder="Password" />
            <span style="color:red" class="error">Valid Password Requiere</span>
        </div>
    </form>
</div>
<div>
    <a href="#" class="btn btn-primary" onclick="ValidationLoginUser();">Login</a>
    @*<button type="button" class="btn btn-primary" id="Login" onclick="LoginUser();">Login</button>*@
    <a href="http://localhost:25250/Registers" class="btn">Register</a>
</div>

@section Scripts {
    <script src="~/Scripts/Sweetalert/sweetalert.min.js"></script>
    <link href="~/Scripts/Sweetalert/sweetalert.css" rel="stylesheet" />
    <script src="~/Scripts/LoginMaster/LoginUser.js"></script>
    
    @Scripts.Render("~/bundles/jqueryval")
}